<?xml version="1.0" encoding="Windows-1252"?>
<xs:schema targetNamespace="MusicArtDownloader.Data.Fanart.Generated"
           attributeFormDefault="unqualified" elementFormDefault="qualified" 
           xmlns:my="MusicArtDownloader.Data.Fanart.Generated"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="fanart" type="my:Fanart"/>

  <xs:complexType name="Fanart">
    <xs:sequence>
      <xs:element name="music" type="my:Music"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Music">
    <xs:all>
      <xs:element name="artistbackgrounds" type="my:ArtistBackgrounds" minOccurs="0" maxOccurs="1"/>
      <xs:element name="albums" type="my:Albums"/>
      <xs:element name="artistthumbs" type="my:ArtistThumbs" minOccurs="0" maxOccurs="1"/>
      <xs:element name="musiclogos" type="my:MusicLogos" minOccurs="0" maxOccurs="1"/>
      <xs:element name="hdmusiclogos" type="my:HDMusicLogos" minOccurs="0" maxOccurs="1"/>
      <xs:element name="musicbanners" type="my:MusicBanners" minOccurs="0" maxOccurs="1"/>
    </xs:all>
    <xs:attribute name="id" type="xs:string" use="required"/>
    <xs:attribute name="name" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="ArtistBackgrounds">
    <xs:sequence>
      <xs:element name="artistbackground" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attributeGroup ref="my:ArtAttributes"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Albums">
    <xs:sequence>
      <xs:element name="album" type="my:Album" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Album">
    <xs:sequence>
      <xs:element name="cdart" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attributeGroup ref="my:CDArtAttributes"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="albumcover" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attributeGroup ref="my:ArtAttributes"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="ArtistThumbs">
    <xs:sequence>
      <xs:element name="artistthumb" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attributeGroup ref="my:ArtAttributes"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MusicLogos">
    <xs:sequence>
      <xs:element name="musiclogo" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attributeGroup ref="my:ArtAttributes"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="HDMusicLogos">
    <xs:sequence>
      <xs:element name="hdmusiclogo" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attributeGroup ref="my:ArtAttributes"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="MusicBanners">
    <xs:sequence>
      <xs:element name="musicbanner" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attributeGroup ref="my:ArtAttributes"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:attributeGroup name="ArtAttributes">
    <xs:attribute name="id" type="xs:int" use="required"/>
    <xs:attribute name="url" type="xs:anyURI" use="required"/>
    <xs:attribute name="likes" type="xs:int" use="required"/>
  </xs:attributeGroup>

  <xs:attributeGroup name="CDArtAttributes">
    <xs:attributeGroup ref="my:ArtAttributes"/>
    <xs:attribute name="disc" type="xs:int" use="required"/>
    <xs:attribute name="size" type="xs:int" use="required"/>
  </xs:attributeGroup>
</xs:schema>